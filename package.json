		"sass": "^1.56.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.6",
		"ts-jest": "^29.0.3",
